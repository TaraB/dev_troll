<h1>Listing devs</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @devs.each do |dev| %>
      <tr>
        <td><%= dev.name %></td>
        <td><%= link_to 'Show', dev %></td>
        <td><%= link_to 'Edit', edit_dev_path(dev) %></td>
        <td><%= link_to 'Destroy', dev, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dev', new_dev_path %>
